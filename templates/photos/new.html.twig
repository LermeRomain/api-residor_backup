{% extends 'base.html.twig' %}

{% block title %}New Photos{% endblock %}

{% block body %}
    <div class="navbar">{% include ('/navbar/index.html.twig') %}</div>
    <div class="container">
        <h1>Ajouter une photo</h1>

        {{ form_start(form) }}
        <div class="form-group">
            {{ form_row(form.imageFile) }}
        </div>

        <div class="form-group">
            {{ form_row(form.idLogements) }}
        </div>

        <button type="submit" class="btn btn-primary mt-5">Enregistrer</button>
        {{ form_end(form) }}

    </div>
{% endblock %}
